        <record id="sequence_asset_type" model="ir.sequence.type">
            <field name="name">Account Asset</field>
            <field name="code">account.asset.code</field>
        </record>
        <record id="sequence_asset" model="ir.sequence">
            <field name="name">Account Asset</field>
